package arrays;

public class BubbleSort {
	public static void main(String[] args) {


		int a[] = {10,3,20,44,23,999,45,3,66};

		int i,j;

		for(i=0; i<a.length; i++){
			for(j=i+1; j<a.length;j++){              //                  for(j=0; j<n-i-1;j++){
				if(a[j]<a[i]){
					int temp =a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
		} 
		for(int k: a){
			System.out.print(k+" ");
		}
	}
}
